<?php

namespace LSUser\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * UserCategoryTicket
 *
 * @ORM\Table(name="user_category_ticket")
 * @ORM\Entity
 */
class UserCategoryTicket
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var \CategoryTicket
     *
     * @ORM\ManyToOne(targetEntity="CategoryTicket")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="category_ticket_id", referencedColumnName="id")
     * })
     */
    private $categoryTicket;

    /**
     * @var \User
     *
     * @ORM\ManyToOne(targetEntity="User")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="user_id", referencedColumnName="id")
     * })
     */
    private $user;


}
